<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>Codexus.Game.Launcher</AssemblyName>
    <GenerateAssemblyInfo>False</GenerateAssemblyInfo>
    <TargetFramework>net10.0</TargetFramework>
    <Copyright>Nirvana And Codexus</Copyright>
    <Version>1.0.0.0</Version>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
  </PropertyGroup>
  <PropertyGroup>
    <LangVersion>14</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup />
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DebugType>embedded</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="Codexus.Cipher">
      <HintPath>..\SDK\Codexus.Cipher.dll</HintPath>
    </Reference>
    <Reference Include="Codexus.Development.SDK">
      <HintPath>..\SDK\Codexus.Development.SDK.dll</HintPath>
    </Reference>
    <Reference Include="Codexus.Interceptors">
      <HintPath>..\SDK\Codexus.Interceptors.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Serilog" Version="4.3.1-dev-02395" />
    <PackageReference Include="Serilog.Sinks.Console" Version="6.1.1" />
    <PackageReference Include="Serilog.Sinks.Console.LogThemes" Version="0.4.0" />
    <PackageReference Include="SharpCompress" Version="0.42.0" />
    <PackageReference Include="System.Management" Version="10.0.1" />
  </ItemGroup>
</Project>